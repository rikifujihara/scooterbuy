<div>
<% if @listing.picture.attached? %>
  <%= image_tag @listing.picture, alt: @listing.title %>
<% else %>
  <%= image_tag "data:image/jpeg;base64,", alt: 'placeholder' %>
<% end %>
</div>

<p>
  <strong>Title:</strong>
  <%= @listing.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @listing.description %>
</p>

<p>
  <strong>Price:</strong>
  <%= @listing.price %>
</p>

<p>
  <strong>User:</strong>
  <%= @listing.user_id %>
</p>

<p>
  <strong>Sold:</strong>
  <%= @listing.sold %>
</p>

<%= link_to 'Edit', edit_listing_path(@listing) %> |
<%= link_to 'Back', listings_path %>
